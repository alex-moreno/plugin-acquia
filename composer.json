{
  "name": "drutiny/acquia",
  "type": "library",
  "description": "Acquia specific policies and audits for Drutiny.",
  "keywords": ["drupal", "audit", "drush", "ssh", "report"],
  "license": "GPL-2.0",
  "authors": [
    {"name": "Sean Hamlin", "email": "iwiifm@gmail.com"},
    {"name": "Josh Waihi", "email": "joshwaihi@gmail.com"}
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "drutiny/drutiny": "2.3.*@dev",
    "guzzlehttp/guzzle": "~6.0",
    "kevinrob/guzzle-cache-middleware": "^3.2.1",
    "drutiny/plugin-drupal-8": "2.x-dev",
    "drutiny/plugin-drupal-7": "2.x-dev",
    "drutiny/http": "2.x-dev",
    "acquia/http-hmac-php": "^4.0"
  },
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/drutiny/drutiny-acquia/issues",
    "source": "https://github.com/drutiny/drutiny-acquia"
  },
  "autoload": {
    "psr-4": {
      "Drutiny\\Acquia\\": "src/",
      "DrutinyTests\\Acquia\\": "tests/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  }
}
